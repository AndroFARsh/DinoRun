.root_container {
    position:absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background-color: rgb(255, 255, 255);
    padding-top: 10%;
    padding-bottom: 10%;
}

.content {
    flex-grow: 1; 
    background-color: rgb(135, 135, 135); 
    justify-content: flex-start; 
    flex-direction: column-reverse;
}

.progress {
    height: 5px; 
    flex-direction: column-reverse;
}